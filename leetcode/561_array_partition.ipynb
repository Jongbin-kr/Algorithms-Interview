    "class Solution(object):\n",
    "    def arrayPairSum(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        \n",
    "        nums.sort()\n",
    "\n",
    "        result = 0\n",
    "\n",
    "        while nums:\n",
    "            result += min(nums.pop(), nums.pop())\n",
    "\n",
    "        return result"
   ]
