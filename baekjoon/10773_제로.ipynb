    "K = int(input())\n",
    "\n",
    "stack = []\n",
    "for _ in range(K):\n",
    "    n = int(input())\n",
    "             \n",
    "    if n: stack.append(n)\n",
    "    else: stack.pop()\n",
    "    \n",
    "print(sum(stack))"
