   "source": [
    "N = int(input())\n",
    "\n",
    "d = [0] * (N+1)\n",
    "\n",
    "\n",
    "for i in range(1, N+1):\n",
    "    if i == 1:\n",
    "        d[i] = 1\n",
    "        continue\n",
    "    \n",
    "    if i == 2:\n",
    "        d[i] = 2\n",
    "        continue\n",
    "    \n",
    "    d[i] = (d[i-1] + d[i-2]) % 15746\n",
    "    \n",
    "print(d[N])"
   ]
  }
