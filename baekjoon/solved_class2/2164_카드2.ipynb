   "source": [
    "deque 사용해서 그냥 그대로 구현?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4\n"
     ]
    }
   ],
   "source": [
    "from collections import deque\n",
    "q = deque(range(1, int(input())+1))\n",
    "\n",
    "while len(q) != 1:\n",
    "    q.popleft()     ## 1. 제일 위에 있는 카드 버리고,\n",
    "    card = q.popleft()         ## 2. 다음 위에 있는 카드를 맨 밑으로 옮긴다\n",
    "    q.append(card)\n",
    "else:\n",
    "    print(q.pop())"
   ]
  }
 ],
